@page "/models/{ModelName}"
@inject NavigationManager Navigation
<h3>Models</h3>

<p>Model Name: @ModelName</p>
<button class="btn btn-danger mb-3" @onclick="@(() => Navigation.NavigateTo(" /models/dall-e"))">Switch to Dall-E Model</button>
<ChatGpt />

}
else
{
<button class="btn btn-danger mb-3" @onclick="@(() => Navigation.NavigateTo(" /models/chatgpt"))">Switch to ChatGPT Model</button>
<DallE />

}
@code {
    [Parameter]
    public string ModelName { get; set; }
}

<!--
Sayfaya parametre eklemek için {} kullanılır. Eğer string değilse ModelName:int şeklinde tanımlanır.

-->
